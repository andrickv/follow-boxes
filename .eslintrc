{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "no-shadow": 0,
        "no-loop-func": 0,
        "no-plusplus": 0,
        "no-console": 0,
        "react/prop-types": 0
    }
}